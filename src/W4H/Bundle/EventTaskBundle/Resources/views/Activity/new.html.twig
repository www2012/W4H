{% extends "W4HCalendarBundle:Admin:base.html.twig" %}

{% block content %}

<h1>Activity creation</h1>

<form action="{{ path('activity_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('activity') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
