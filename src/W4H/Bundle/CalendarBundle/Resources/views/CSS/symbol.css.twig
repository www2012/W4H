<style type="text/css">

  {% for slug, bg in events %}
    body .task.{{ slug }},
    body .{{ slug }} > .symbol
    {
        border-radius: 8px !important;
        border: 2px solid {{ bg }} !important; 
    }
  {% endfor %}

  {% for slug, bg in activity_types %}
    .task.{{ slug }},
    .{{ slug }} > .symbol
    {
        background: {{ bg }} !important;
    }
  {% endfor %}

</style>
