{% extends "W4HCalendarBundle:Admin:base.html.twig" %}

{% block content %}

<h1>Location</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Building</th>
            <td>{{ entity.building }}</td>
        </tr>
        <tr>
            <th>Level</th>
            <td>{{ entity.level }}</td>
        </tr>
        <tr>
            <th>Classe_room_places</th>
            <td>{{ entity.classeroomplaces }}</td>
        </tr>
        <tr>
            <th>Conference_room_places</th>
            <td>{{ entity.conferenceroomplaces }}</td>
        </tr>
        <tr>
            <th>Standing_room_places</th>
            <td>{{ entity.standingroomplaces }}</td>
        </tr>
        <tr>
            <th>Video_projector</th>
            <td>{{ entity.videoprojector }}</td>
        </tr>
        <tr>
            <th>Sound</th>
            <td>{{ entity.sound }}</td>
        </tr>
        <tr>
            <th>Internet</th>
            <td>{{ entity.internet }}</td>
        </tr>
        <tr>
            <th>Other_devices</th>
            <td>{{ entity.otherdevices }}</td>
        </tr>
        <tr>
            <th>Accessibility</th>
            <td>{{ entity.accessibility }}</td>
        </tr>
        <tr>
            <th>Latitude</th>
            <td>{{ entity.latitude }}</td>
        </tr>
        <tr>
            <th>Longitude</th>
            <td>{{ entity.longitude }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('location') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('location_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('location_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
